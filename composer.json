{
  "name": "nettrine/annotations",
  "description": "Doctrine Annotations for Nette Framework",
  "keywords": ["nettrine", "nette", "doctrine", "annotations"],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/contributte/doctrine-annotations",
  "authors": [
    {
      "name": "Milan Felix Å ulc",
      "homepage": "https://f3l1x.io"
    }
  ],
  "require": {
    "php": ">=8.2",
    "nette/di": "^3.2.4",
    "doctrine/annotations": "^2.0.2",
    "nettrine/cache": "^0.5.0 || ^0.6.0"
  },
  "require-dev": {
    "contributte/phpstan": "^0.2",
    "contributte/qa": "^0.4",
    "contributte/tester": "^0.3",
    "symfony/cache": "^7.2.1"
  },
  "autoload": {
    "psr-4": {
      "Nettrine\\Annotations\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.10.x-dev"
    }
  }
}
