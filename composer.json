{
  "name": "nettrine/annotations",
  "description": "Doctrine Annotations for Nette Framework",
  "keywords": ["nettrine", "nette", "doctrine", "annotations"],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/contributte/doctrine-annotations",
  "authors": [
    {
      "name": "Milan Felix Å ulc",
      "homepage": "https://f3l1x.io"
    }
  ],
  "require": {
    "php": ">=7.2",
    "contributte/di": "^0.5.0",
    "doctrine/annotations": "^1.6.1",
    "nettrine/cache": "^0.3.0"
  },
  "require-dev": {
    "ninjify/nunjuck": "^0.4",
    "ninjify/qa": "^0.13",
    "phpstan/phpstan": "^0.12.0",
    "phpstan/phpstan-deprecation-rules": "^0.12.0",
    "phpstan/phpstan-nette": "^0.12.0",
    "phpstan/phpstan-strict-rules": "^0.12.0"
  },
  "autoload": {
    "psr-4": {
      "Nettrine\\Annotations\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Cases\\": "tests/cases",
      "Tests\\Fixtures\\": "tests/fixtures",
      "Tests\\Toolkit\\": "tests/toolkit"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.8.x-dev"
    }
  }
}
